<div class="votes_size"><strong>Score: </strong><%= project.likes.size - project.dislikes.size %></div>
<div class="vote">
  <%= link_to "Upvote", project_like_path(project), method: :post, data: { 'vote-button' => 'create' }, remote: true %></div>
<div class="vote">
  <%= link_to "Downvote", project_dislike_path(project), method: :post, data: { 'vote-button' => 'create' }, remote: true %></div>
